using System;

namespace Task1
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Input a number for the shape/sides you want");
            string[] arr = {"circle","semi-circle","triangle","square",
                                 "pentagon","hexagon","heptagon","octagon",
                                 "nonagon","decagon"};
            int num = int.Parse(Console.ReadLine());
            Console.WriteLine($"{arr[num - 1]}");

                
           
        }
        
    }
}
